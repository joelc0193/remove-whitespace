{
  "name": "remove-whitespace",
  "displayName": "Remove whitespace",
  "description": "Leaves only minimum necessary whitespace for Flutter to work",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:remove-whitespace"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "remove-whitespace",
        "title": "Remove Whitespace"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joelc0193/remove-whitespace.git"
  },
  "publisher": "JoelCastro",
  "dependencies": {
    "clipboardy": "^3.0.0"
  }
}
